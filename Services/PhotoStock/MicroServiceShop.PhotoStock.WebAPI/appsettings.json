{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",

  "CloudinarySettings": {
    "CloudName": "your_CloudName",
    "ApiKey": "your_ApiKey",
    "ApiSecret": "your_ApiSecret"
  },
  "IdentityServerURL": "http://localhost:5001",
  "ElasticConfiguration": {
    "Uri": "http://localhost:9200"
  },
  "ApplicationName": "photostock.api",
  "EnvironmentName": "Development",
  "IndexName": "photostock.api",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      }
    ]
  },
  "RabbitMQUrl": "127.0.0.1"
}
